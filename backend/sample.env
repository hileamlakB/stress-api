# Supabase Authentication (required)
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_key
SUPABASE_JWT_SECRET=your_jwt_secret

# Database (use ONE of these options)
# Option 1: Direct connection (requires IPv6 or IPv4 add-on)
SUPABASE_DB_URL=postgresql://postgres:password@db.your-project-ref.supabase.co:5432/postgres

# Option 2: Session pooler (works with IPv4, recommended for most cases)
SUPABASE_DB_SESSION_POOLER=postgresql://postgres.your-project-ref:password@aws-0-region.pooler.supabase.com:5432/postgres

# App settings
FRONTEND_URL=http://localhost:5173
BACKEND_URL=http://localhost:8000

# Debug settings
DEBUG=False 